<Window x:Class="TMtextng.Redewendung.GroupViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TMtextng.Redewendung"
        xmlns:properties="clr-namespace:TMtextng.Properties"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="GroupViewWindow" Height="800" Width="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*" />
            <RowDefinition Height="2*" />
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" x:Name ="GroupButtonsGrid">
        </Grid>

        <Grid Grid.Row="1" x:Name="BackButtonGrid" >
        </Grid>
    </Grid>
</Window>
